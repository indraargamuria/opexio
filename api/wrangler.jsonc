    }
  ],
  "env": {
    "production": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "opexio-db",
          "database_id": "63bdbe86-a44d-4160-9299-1426249e0512",
          "migrations_dir": "drizzle"
        }
      ],
      "r2_buckets": [
        {
          "binding": "MY_BUCKET",
          "bucket_name": "opexio-shipment-files"
        }
      ]
    }
  }
}